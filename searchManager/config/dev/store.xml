        <module name="settings" title="Settings">
            <groups>
                <group name="Banner Ads Feature">
                    <member property-id="site_domain"/>
                    <member property-id="default_banner_linkpath_protocol"/>
                    <member property-id="autoprefix_banner_linkpath_protocol"/>
